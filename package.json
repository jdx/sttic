{
  "name": "sttic",
  "version": "0.0.1",
  "author": "Jeff Dickey @jdxcode",
  "bin": {
    "sttic": "./bin/run"
  },
  "bugs": "https://github.com/jdxcode/statc/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "aws-sdk": "^2.256.1",
    "cli-ux": "^4.6.3",
    "mime-types": "^2.1.18",
    "qqjs": "^0.3.10",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/execa": "^0.9.0",
    "@types/glob": "^5.0.35",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/write-json-file": "^2.2.1",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^12",
    "ts-node": "^6",
    "tslint": "^5",
    "typescript": "^2.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jdxcode/statc",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "statc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jdxcode/statc",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc",
    "test": "echo NO TESTS -- nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
